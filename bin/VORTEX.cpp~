#include <stdlib.h>
#include <stdio.h>

int modInverse(int a, int m) 
{ 
    a = a%m; 
    for (int x=1; x<m; x++) 
       if ((a*x) % m == 1) 
          return x; 
}

int encrypt(int content, int key)
{
  int payload = content % key;
}

int decrypt(int payload, int key)
{
  int plain = modInverse(payload, key)
}

int main()
{
  int payload = encrypt(atoi(argv[1]), 920);
  int plain = decrypt(payload, 920);

  printf("ENCRYPTING: %s\nCIPHERTEXT: %s\nPLAINTEXT: %s\n", argv[1], payload, plain);
}
