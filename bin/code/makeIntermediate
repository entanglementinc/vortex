#!/bin/bash
function intermediates()
{
	echo "=== $1 ==="
	echo -n abcdefghijklmnopqrstuvwxyz | $2
	echo ""
	echo ""
}
(
echo "Intermediate states of the hashing functions applied to the string \"abcdefghijklmnopqrstuvwxyz\""
echo ""
intermediates "FSB-160"  "int/fsb160"
intermediates "FSB-224"  "int/fsb224"
intermediates "FSB-256"  "int/fsb256"
intermediates "FSB-384"  "int/fsb384"
intermediates "FSB-512"  "int/fsb512"
intermediates "SFSB-160" "int/sfsb160"
intermediates "SFSB-224" "int/sfsb224"
intermediates "SFSB-384" "int/sfsb384"
intermediates "RFSB-509" "int/rfsb"
) > intermediates
